{
  "name": "atk4/ui",
  "type": "library",
  "description": "Agile UI - Web Component Framework written in PHP",
  "keywords": [
    "framework",
    "ui",
    "web",
    "form",
    "component",
    "widget",
    "render",
    "gadget",
    "button",
    "grid",
    "crud"
  ],
  "homepage": "https://agiletoolkit.org/ui",
  "license": "MIT",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Romans Malinovskis",
      "email": "romans@agiletoolkit.org",
      "homepage": "https://nearly.guru/"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "atk4/data": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "<6",
    "phpunit/phpcov": "^3.0",
    "codeclimate/php-test-reporter": "*",
    "behat/behat": "^3.4",
    "behat/mink-selenium2-driver": "^1.3",
    "behat/mink-extension": "^2.2",
    "fzaninotto/faker": "*",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/process": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "atk4\\ui\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atk4\\ui\\tests\\": "tests/"
    }
  }
}
